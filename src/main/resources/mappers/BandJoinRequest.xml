<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.BandJoinRequest">

    <select id="countMyNoticeById" parameterType="string" resultType="int">
        select count(*) as noticeCnt from bandJoinRequest where bandNo in (select no from band where masterId=#{id})
    </select>




</mapper>